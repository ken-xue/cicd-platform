<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.kenxue.cicd.infrastructure.repositoryimpl.application.database.mapper.MachineInfoMapper">
    <resultMap type="io.kenxue.cicd.infrastructure.repositoryimpl.application.database.dataobject.MachineInfoDO" id="machineInfoMap">
            <result property="ip" column="ip"/>
            <result property="name" column="name"/>
            <result property="port" column="port"/>
            <result property="accessUsername" column="access_username"/>
            <result property="accessPassword" column="access_password"/>
            <result property="accessKey" column="access_key"/>
    </resultMap>
    <select id="queryPage"
            resultType="io.kenxue.cicd.infrastructure.repositoryimpl.application.database.dataobject.MachineInfoDO">
            select distinct(machine_info.uuid) machine_info_uuid,machine_info.* from machine_info left join machine_of_group on machine_info.uuid = machine_of_group.machine_uuid
             where 1 = 1
            <if test="qry.machineInfoDTO.groupUuid != null and qry.machineInfoDTO.groupUuid != ''">
                and machine_of_group.group_uuid = #{qry.machineInfoDTO.groupUuid}
            </if>
            order by gmt_create
    </select>

</mapper>